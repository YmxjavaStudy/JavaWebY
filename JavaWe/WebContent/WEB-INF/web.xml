<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>JavaWe</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>ServeletDemo1</servlet-name>
    <servlet-class>com.ymx.javaweb.servlet.ServeletDemo1</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServeletDemo1</servlet-name>
    <url-pattern>/ServeletDemo1</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>MyloginServlet</servlet-name>
    <servlet-class>com.ymx.javaweb.servlet.MyloginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MyloginServlet</servlet-name>
    <url-pattern>/servlet/MyloginServlet</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>InitServletDemo</servlet-name>
    <servlet-class>com.ymx.javaweb.servlet.InitServletDemo</servlet-class>
    <load-on-startup>1</load-on-startup>
    <init-param>
      <param-name>abc</param-name>
      <param-value>安保处</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>InitServletDemo</servlet-name>
    <url-pattern>/servlet/InitServletDemo</url-pattern>
  </servlet-mapping>
</web-app>